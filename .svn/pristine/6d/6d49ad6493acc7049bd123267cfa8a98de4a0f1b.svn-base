body, form, div, p, a, img, h1, h2, h3, h4, h5, h6, table, table tr, table td {
    border: none;
    color: #000000;
    font-family: 'Noto Sans', sans-serif;
    font-size: 13px;
    font-style: normal;
    font-weight: normal;
    margin: 0;
    padding: 0;
}

body {
    padding: 10px;
    line-height: 20px;
}

h1, h2 {
    font-size: 21px;
    color: #0cb061;
    font-weight: bold;
}

p {
    margin: 0 0 10px 0;
}