.ob-block-company-structure {
    list-style: none;
    white-space: nowrap;
    margin: 0;
    padding: 0;
    position: relative;

    ul {
        list-style: none;
        white-space: nowrap;
        margin: 0 0 0 9px;
        padding: 0 0 0 9px;
        overflow: hidden;
        position: relative;
        letter-spacing: -4px;

        &.last {
            white-space: normal;

            li {
                display: block;
            }
        }
    }

    li {
        margin: 10px 10px 10px 0;
        display: inline-block;
        vertical-align: top;
        letter-spacing: 0;
    }

    .role {
        position: relative;
        white-space: normal;
        width: 250px;

        &:before {
            border-bottom: 1px solid $colorBorderBase;
            top: -977px;
            content: "";
            height: 1000px;
            left: -1000px;
            position: absolute;
            transition-duration: 0.3s;
            width: 1000px;
        }

        .wrap {
            background-color: #ffffff;
            border: 1px solid $colorBorderBase;
            padding: 5px 10px;
            position: relative;
            z-index: 1;

            &:hover {
                border-color: $colorLinkBase;
            }
        }

        strong {
            display: block;
        }

        a {
            font-size: $fontSizeBase - 2;
        }
    }

    li:first-child > .role,
    .last .role {
        &:before {
            border-bottom: 1px solid $colorBorderBase;
            border-left: 1px solid $colorBorderBase;
            top: -977px;
            content: "";
            height: 1000px;
            left: -9px;
            position: absolute;
            transition-duration: 0.3s;
            width: 8px;
        }
    }
}

