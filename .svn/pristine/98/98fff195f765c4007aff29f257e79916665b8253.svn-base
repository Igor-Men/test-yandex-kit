    .empty {
        line-height: 0;
        display: inline-block;
        width: 120px;
        height: 0;
        vertical-align: top;
    }

    .ui-draggable-dragging {
        z-index: 1;
    }

    @media screen and (min-width: 1440px){
        &.line {
            -moz-column-count: 2;
            -webkit-column-count: 2;
            column-count: 2;
            -moz-column-gap: 20px;
            -webkit-column-gap: 20px;
            column-gap: 20px;
        }
