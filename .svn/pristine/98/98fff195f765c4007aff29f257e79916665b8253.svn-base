    }
}

.ob-inbox-buttons {
    line-height: $lineHeightBase + 12;
    margin: 0 0 5px 0;
}

.ob-callnumber-block {
    padding: 0 0 5px 0;

    [type="text"] {
        float: left;
        width: 200px;
    }

    [type="button"] {
        float: left;
        width: 45px;
        margin: 0 0 0 5px;
    }
}

.ob-orderlist-toggle {
    float: right;
    padding: 5px 10px;
    background-color: $colorBorderBase;
    color: #555555;
    font-size: $fontSizeBase - 2;

    a {
        font-size: inherit;
    }
}

.ob-gantt-table {
    border: 1px solid #d3d6dc;
    position: relative;
    margin: 0 0 20px 0;

    .row {
        height: 20px;
        padding: 0 0 0 300px;
        position: relative;
        @include box-sizing($bb);

        &:hover {
            background-color: #dbe6f4;
            padding: 0 0 0 300px;
        }
    }

    .issue-block {
        position: absolute;
        top: 0;
        left: 0;
        width: 301px;
        line-height: $lineHeightBase + 2;
        padding: 0 5px;
        font-size: $fontSizeBase - 3;
        @include box-sizing($bb);
        border-right: 1px solid #d3d6dc;
        cursor: move;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;

         a {
            font-size: $fontSizeBase - 3;
        }

        .mark {
            padding: 1px 2px;
        }

        [type="checkbox"] {
            display: inline-block;
            vertical-align: middle;
            margin: 0 5px 0 0;
        }
    }

    .value-block {
        height: 20px;
        overflow: hidden;
    }

    .days-list {
        height: 20px;
        background-image: url('/_images/admin/gantt/days-bg.png');
        position: relative;
    }

    .day,
    .month-28,
    .month-29,
    .month-30,
    .month-31 {
        float: left;
        width: 20px;
        height: 20px;
        line-height: $lineHeightBase + 2;
        text-align: center;
        font-size: $fontSizeBase - 3;
        border-left: 1px solid #d3d6dc;
        @include box-sizing($bb);
    }

    .day {
        width: 20px;
    }

    .month-28 {
        width: 560px;
    }

    .month-29 {
        width: 580px;
    }

    .month-30 {
        width: 600px;
    }

    .month-31 {
        width: 620px;
    }

    .scroll-row {
        position: absolute;
        width: 100%;
        bottom: -20px;
        left: 0;

        &:hover {
            background-color: transparent !important;
        }

        .days-list {
            background-image: none;
        }

        .value-block {
            position: relative;
            overflow: hidden;
        }

        .ps-scrollbar-x-rail {
            opacity: 1 !important;
        }
    }

    .header {
        background-color: #f3f3f3 !important;
        border-bottom: 1px solid #d3d6dc;

        .days-list {
            background-image: none;
        }

        .no-border {
            background-color: #f3f3f3;
            top: -1px;
        }

        .issue-block {
            cursor: default;
        }
    }

    .sortable-rows,
    .sortable-rows ol,
    .sortable-rows li {
        padding: 0;
        margin: 0;
        list-style: none;
    }

    .sortable-rows {
        li .issue-block {
            padding-left: 25px;
        }

        li li .issue-block {
            padding-left: 45px;
            background-image: url('/_images/admin/gantt/tree-element.png');
            background-position: 30px center;
            background-repeat: no-repeat;
        }

        li li li .issue-block {
            padding-left: 65px;
            background-position: 50px center;
        }

        li li li li .issue-block {
            padding-left: 85px;
            background-position: 70px center;
        }

        li li li li li .issue-block {
            padding-left: 105px;
            background-position: 90px center;
        }

        .placeholder {
            background-color: #fcf7a7;
        }

        li .placeholder {
            border-left: 25px solid #ffffff;
        }

        li li .placeholder {
            border-left: 45px solid #ffffff;
        }

        li li li .placeholder {
            border-left: 65px solid #ffffff;
        }

        li li li li .placeholder {
            border-left: 85px solid #ffffff;
        }
    }

    .line {
        height: 20px;
        position: absolute;
        top: 0;
        left: 0;
        box-shadow: 0 0 0 1px rgba(0, 0, 0, 0.1) inset;

        &:hover {
            background-image: url('/_images/admin/gantt/move-bg.png');
            background-position: center center;
            background-repeat: no-repeat;
            cursor: col-resize;
        }

        &:hover .ui-resizable-w:after {
            background-image: url('/_images/admin/gantt/resize.png');
            background-position: left center;
            background-repeat: no-repeat;
            width: 4px;
            height: 6px;
            position: absolute;
            content: '';
            left: 6px;
            top: 7px;
        }

        &:hover .ui-resizable-e:after {
            background-image: url('/_images/admin/gantt/resize.png');
            background-position: right center;
            background-repeat: no-repeat;
            width: 4px;
            height: 6px;
            position: absolute;
            content: '';
            right: 6px;
            top: 7px;
        }

        &.no-edit {
            height: 10px;
            top: 5px;
            cursor: default;
            background-image: none !important;

            .ui-resizable-w,
            .ui-resizable-e {
                display: none !important;
            }
        }
    }

    .expand {
        position: absolute;
        width: 10px;
        height: 10px;
        left: 5px;
        top: 5px;
        cursor: pointer;
        background: url('/_images/admin/plus-minus.png') no-repeat;
        display: none;

        &:hover {
            background-position: top right;
        }
    }

    .branched  {
        & > .row .expand {
            display: block;
        }
    }

    .collapsed {
        ol {
            display: none;
        }
    }

    .expanded {
        & > .row .expand {
            background-position: bottom left;

            &:hover {
                background-position: bottom right;
            }
        }
    }

    ::-webkit-scrollbar {
        background-color: rgba($colorLinkBase, 0.2);
        -webkit-appearance: none;
        height: 10px;
    }

    ::-webkit-scrollbar-thumb {
        height: 10px;
        border-radius: 4px;
        background-color: $colorLinkBase;
        -webkit-box-shadow: 0 0 1px rgba(255,255,255, 0.5);
    }
}

.ob-funnel-container {
    width: 300px;
    float: left;
    margin-right: 10px;

    .funnel-block {
        color: white;
        margin-bottom: 1px;
        text-align: center;

        &:nth-child(2) {
            padding: 0 8px;
        }

        &:nth-child(3) {
            padding: 0 16px;
            margin: 0 0 10px 0;
        }

        &:nth-child(4) {
            padding: 0 24px;
        }

        &:nth-child(5) {
            padding: 0 32px;
        }

        &:nth-child(6) {
            padding: 0 40px;
        }

        &:nth-child(7) {
            padding: 0 48px;
        }

        &:nth-child(8) {
            padding: 0 56px;
        }

        &:nth-child(9) {
            padding: 0 64px;
        }

        &:nth-child(10) {
            padding: 0 72px;
        }

        span {
            display: block;
            background: url('/_images/admin/funnel-l.png') top left no-repeat #808080;

            span {
                display: block;
                background: url('/_images/admin/funnel-r.png') top right no-repeat;
                padding: 0 10px;
                height: 35px;
                line-height: $lineHeightBase + 17;
            }
        }

        &:nth-child(3) span span {
