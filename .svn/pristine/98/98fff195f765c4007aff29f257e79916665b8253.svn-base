        background: -webkit-gradient(linear, left top, right top, color-stop(0%,#000000), color-stop(100%,#696969));
        background: -webkit-linear-gradient(left, #000000 0%,#696969 100%);
        background: -o-linear-gradient(left, #000000 0%,#696969 100%);
        background: -ms-linear-gradient(left, #000000 0%,#696969 100%);
        background: linear-gradient(to right, #000000 0%,#696969 100%);
