.ob-block-widgets {
    padding: 0;
    margin: 0 0 0 -20px;
    list-style: none;

    &:after {
        content: '';
        clear: both;
        display: block;
        height: 0;
    }

    li {
        float: left;
        width: 50%;
        margin: 0 0 20px 0;
        padding: 0 0 0 20px;
        @include box-sizing($bb);

        &.double {
            width: 100%;
        }

        &.placeholder {
            height: 100px;
            position: relative;
            
            &:before {
                background-color: #fff8b7;
                content: '';
                width: 100%;
                height: 100%;
                display: block;
                border: 1px dashed #cccccc;
            }
        }
    }

    .widget-element {
        &:hover {
            outline: 5px solid $colorBorderBase;
        }

        .widget-caption {
            background-color: $colorBorderBase;
            padding: 10px;
            cursor: move;
        }

        .widget-content {
            min-height: 100px;
            background-color: rgba($colorBorderBase, 0.1);

            &.new {
                background-image: url('/_images/admin/icon-add.png');
                background-position: center center;
                background-repeat: no-repeat;
                cursor: pointer;
            }
        }

        .shop-calendar-block {
            margin: 0;
        }
    }

    .disabled {
        .widget-element:hover {
            outline: none;
