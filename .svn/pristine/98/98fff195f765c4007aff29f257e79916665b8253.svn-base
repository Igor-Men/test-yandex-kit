            text-align: center;
            border-top: 1px solid #cccccc;
            background-color: #ffffff;
            height: 20px;
            display: none;
        }

        &.selected label {
            border-top: 1px solid $colorLinkBase;
            background-color: $colorLinkBase;
        }

        &:hover label,
        &.selected label {
            display: block;
        }
