    }

    .date {
        float: right;
        color: #898989;
        margin: 0 0 0 20px;
    }
}

.ob-list-chain {
    .element:first-child {
        background-position: -40px center;
    }

    .element:nth-child(2) {
        background-position: -20px center;
    }

    .element:last-child {
