        background-color: $bodyBg;

        &:hover {
            box-shadow: none;
        }
    }

    .by-day {
        .day {
            &:hover {
                box-shadow: none;
            }

            .description {
                display: block;
                overflow: hidden;
                line-height: 14px;
                max-height: 28px;
                font-size: $fontSizeBase - 4;
            }
        }

        .issues {
            height: 500px;
        }
    }

    .by-week {
        .issues {
            height: 500px;
        }
    }

    &.long-list {
        .list {
            width: auto;
            table-layout: auto;
        }

        .day {
            .inner {
                width: 200px;
            }
        }

        .hour-column {
            width: 50px;
        }
    }

    &.no-weekend {
        .by-week {
            .day-name:nth-child(6),
            .day-name:nth-child(7),
            .day:nth-child(6),
            .day:nth-child(7) {
                display: none;
            }
        }

        .by-month {
            .day-name:nth-child(7n+6),
            .day-name:nth-child(7n+7),
            .day:nth-child(7n+6),
            .day:nth-child(7n+7) {
                display: none;
            }
        }
    }
}

