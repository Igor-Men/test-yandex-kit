.ob-welcome-wrap {
    position: relative;
    margin: -10px;
    height: 500px;
    background: #000000;
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(left, #000000 0%, #007ecf 100%);
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,#000000), color-stop(100%,#007ecf));
    background: -webkit-linear-gradient(left, #000000 0%,#007ecf 100%);
    background: -o-linear-gradient(left, #000000 0%,#007ecf 100%);
    background: -ms-linear-gradient(left, #000000 0%,#007ecf 100%);
    background: linear-gradient(to right, #000000 0%,#007ecf 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#000000', endColorstr='#007ecf',GradientType=1 );
    background-size: 200% 200%;
    -webkit-animation: headAnimation 25s ease infinite;
    -moz-animation: headAnimation 25s ease infinite;
    -o-animation: headAnimation 25s ease infinite;
    animation: headAnimation 25s ease infinite;

    .line {
        height: 550px;

