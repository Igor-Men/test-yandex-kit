        }

        &.droppable-hover {
            background-color: #fcf7a7;
        }

        &:hover {
            border: 1px solid #cccccc;
        }

        &.selected {
            border: 1px solid $colorLinkBase;
        }

        .image-folder {
            width: 100px;
            height: 100px;
            background-image: url('/_images/admin/folder-bg.png');
            background-repeat: no-repeat;
            background-position: left top;
            display: block;
            margin: 0 auto;
        }

        &.droppable .image-folder {
            background-position: left center;
        }

        &.droppable-hover .image-folder {
            background-position: center bottom;
        }

        .image-folder.back {
            background-position: center top;
        }

        &.droppable .image-folder.back {
            background-position: center center;
        }

        &.droppable-hover .image-folder.back {
            background-position: center bottom;
        }

        .image-folder.new {
            background-position: right top;
        }

        .image-folder.new-product {
            background-position: right center;
        }

        &.hidden {
            opacity: 0.6;
        }

        .image {
            width: 100px;
            height: 100px;
            margin: 0 auto;
            display: block;

            span {
                width: 100px;
                height: 100px;
                display: table-cell;
                vertical-align: middle;
            }

            img {
                max-width: 100px;
                max-height: 100px;
                display: block;
                margin: 0 auto;
            }
        }

        .name {
            display: block;
            margin: 5px 0;
            line-height: $lineHeightBase - 3;
            max-height: 45px;
            overflow: hidden;
            text-transform: lowercase;

            &:first-letter {
                text-transform: uppercase;
