}

.shop-categorymanage-block {
    margin: 0 0 15px 0;
    padding: 0;
    list-style-type: none;

    .placeholder {
        border: 1px dashed #08f82f;
        background-color: #dbf8cb;
        margin: 0 0 5px 0;
    }

    .shop-admin-nestedSortable-error {
        background: #fbe3e4;
        border: 1px dashed #f8c13d;
        border-color: transparent;
    }

    ol {
        margin: 0 0 0 35px;
        padding: 0;
        list-style-type: none;
    }

    li {
        margin: 5px 0 0 0;
        padding: 0;
        line-height: $lineHeightBase + 9;

        div  {
            margin: 0;
            cursor: move;
            background: #eff0f2;

            &:hover  {
                background: #dbe6f4;
            }

            &.hidden  {
                opacity: 0.6;
            }
        }

        &.shop-admin-nestedSortable-collapsed.shop-admin-nestedSortable-hovering div {
            border-color: #999;
            background: #fafafa;
        }

        &.shop-admin-nestedSortable-collapsed > ol {
            display: none;
        }

        &.shop-admin-nestedSortable-branch > div > .disclose {
            display: inline-block;
        }

        &.shop-admin-nestedSortable-collapsed > div > .disclose > span {
            display: inline-block;
            width: 10px;
            height: 10px;
            background-image: url('/_images/admin/plus-minus.png');
        }

        &.shop-admin-nestedSortable-collapsed > div > .disclose > span:hover {
            background-position: top right;
        }

        &.shop-admin-nestedSortable-expanded > div > .disclose > span {
            display: inline-block;
            width: 10px;
            height: 10px;
            background-image: url('/_images/admin/plus-minus.png');
            background-position: bottom left;
        }

        &.shop-admin-nestedSortable-expanded > div > .disclose > span:hover {
            background-position: bottom right;
        }
    }

    .disclose {
        cursor: pointer;
        width: 13px;
        display: none;
    }

    .edit {
        width: 16px;
        height: 16px;
        text-decoration: none;
        background: url('/_images/admin/icon-edit.png');
        display: inline-block;
        vertical-align: middle;
        margin: 0 10px 0 0;

        &:hover {
            opacity: 0.8;
        }
    }

    .check {
        border-right: 10px solid #ffffff;
        float: left;
        height: 27px;
        margin: 0 10px 0 0;

        input {
            margin: 7px;
        }
    }
}

.shop-categorymanage-add {
