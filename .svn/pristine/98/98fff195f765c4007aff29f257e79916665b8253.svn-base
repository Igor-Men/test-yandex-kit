        .image {
            width: 35px;
            height: 35px;
            display: block;
            position: absolute;
            top: 9px;
            left: 30px;

            span {
                width: 35px;
                height: 35px;
                display: table-cell;
                vertical-align: middle;
            }

            img {
                max-width: 35px;
                max-height: 35px;
            }
        }

        label {
            width: 20px;
            height: 100%;
            border-top: none;
            border-right: 1px solid #cccccc;
            line-height: $lineHeightBase + 40;
        }

        .name {
            line-height: $lineHeightBase + 17;
            height: 35px;
            overflow: hidden;
            white-space: nowrap;
            margin: 0;
            text-overflow: ellipsis;
        }
    }

    &.line .empty {
        display: none;
    }
