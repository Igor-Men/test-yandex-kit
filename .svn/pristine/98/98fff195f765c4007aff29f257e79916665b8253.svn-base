$bodyBg: #f8f8f8;
$colorLinkBase: #007ecf;
$colorSuccess: #0cb061;
$colorImportant: #da3020;
$colorBorderBase: #e1e1e1;
$fontSizeBase: 14px;
$lineHeightBase: 18px;

@mixin box-sizing($boxsizing) {
    -moz-box-sizing: $boxsizing;
    -webkit-box-sizing: $boxsizing;
    box-sizing: $boxsizing;
}
$bb: border-box;

@mixin transition-duration($duration) {
    -webkit-transition-duration: $duration;
    -moz-transition-duration: $duration;
    -o-transition-duration: $duration;
    -ms-transition-duration: $duration;
    transition-duration: $duration;
}

@mixin scale ($scale) {
    -moz-transform: scale($scale, $scale);
    -webkit-transform: scale($scale, $scale);
    -o-transform: scale($scale, $scale);
    -ms-transform: scale($scale, $scale);
    transform: scale($scale, $scale);
}

html,body {
    height: 100%;
    width: 100%;
}

body, form, div, p, a, img, h1, h2, h3, h4, h5, h6, table, table tr, table td {
    border: none;
    color: #000000;
    font-family: Arial;
    font-size: $fontSizeBase;
    font-style: normal;
    font-weight: normal;
    margin: 0;
    padding: 0;
    outline: none;
}

body {
    min-width: 990px;
    background-color: $bodyBg;
}

a {
    text-decoration: underline;
    outline: none;
    color: $colorLinkBase;

    &:hover {
        text-decoration: none;
    }
}

h1 {
    font-size: $fontSizeBase + 4;
    padding: 0 0 5px 0;
    color: #555555;
    font-weight: bold;
}

.h1 {
    font-size: $fontSizeBase + 4;
    padding: 0 0 5px 0;
    font-weight: bold;
}

h2 {
    font-size: $fontSizeBase + 4;
    padding: 0 0 5px 0;
    color: #555555;
}

.h2 {
    font-size: $fontSizeBase + 4;
    padding: 0 0 5px 0;
}

acronym {
    border-bottom: 1px dotted #000000;
}

span.cke_skin_kama {
    padding: 0 !important;
    border: none !important;
}

.cke_skin_kama .cke_wrapper {
    background-image: none !important;
}

.clear {
    clear: both;
    font-size: 0;
    line-height: 0;
    height: 0;
    display: block;
}

.fl-l {
    float: left;
}

.fl-r {
    float: right;
}

.align_right {
    text-align: right;
}

.align_center {
    text-align: center;
}

.va_top {
    vertical-align: top;
}

.va_middle {
    vertical-align: middle;
}

.nowrap {
    white-space: nowrap;
}

.important {
    color: $colorImportant;
}

.good {
    color: $colorSuccess;
}

.layer-table {
    width: 100%;
    border-collapse: collapse;
    color: inherit;

    tbody,
    tr,
    td {
        color: inherit;
    }

    .separator {
        width: 20px;
    }

    .addon {
        width: 200px;
    }
}

@import 'admin_shop_tpl_ui';

.shop-admin-body {
    min-height: 100%;
    height: auto !important;
    height: 100%;
    min-width: 990px;
}

.shop-admin-navi {
    line-height: $lineHeightBase + 22;
    height: 40px;
    background-color: #ffffff;
    position: fixed;
    top: 0;
    z-index: 100;
    width: 100%;
    min-width: 990px;
    box-shadow: 0 0 3px #000000;
    white-space: nowrap;

    .head {
        height: 40px;
    }

    .name {
        display: inline-block;
        vertical-align: top;
        color: #000000;
        font-size: $fontSizeBase + 4;
        height: 40px;
        margin: 0 20px 0 10px;
        position: relative;

        a {
            color: $colorLinkBase;
            font-size: $fontSizeBase + 4;
            text-decoration: none;
            background: url('/_images/admin/logo.png') center left no-repeat;
            padding: 5px 0 5px 30px;
            font-weight: bold;

            &:hover {
                opacity: 0.8;
            }
        }

        sup {
            font-size: $fontSizeBase - 4;
            font-weight: normal;
            position: absolute;
            top: 1px;
        }
    }

    .top-links {
        display: inline-block;
        background-color: #ffffff;

        a {
            color: #000000;
            margin: 0 0 0 10px;
            font-size: $fontSizeBase;
            text-decoration: none;
            display: inline-block;
            vertical-align: top;

            &:hover,
            &.selected {
                color: $colorLinkBase;
            }
        }

        @media screen and (max-width: 1100px){
            a {
                margin: 0 0 0 5px;
            }
        }
    }

    .blind {
        position: absolute;
        z-index: 100;
        left: 0;
        width: 100%;
        line-height: normal;
        top: 40px;
        box-shadow: 0 5px 5px rgba(0, 0, 0, 0.20);
        white-space: normal;

        .inner {
            background-color: $colorLinkBase;
            border-bottom: 1px solid #c5c4c4;
            display: none;

            .inner-wrap {
                padding: 15px 0;
                -moz-column-count: 5;
                -webkit-column-count: 5;
                column-count: 5;
            }
        }

        .list-element {
            padding: 3px 20px;
            line-height: $lineHeightBase - 4;
            display: inline-block;
        }

        a {
            color: #ffffff;
            text-decoration: none;
            font-size: $fontSizeBase;

            &.nowrap {
                white-space: nowrap;
                overflow: hidden;
                text-overflow: ellipsis;
                display: block;
            }

            &:hover {
                text-decoration: underline;
            }
        }
    }

    .search-wrap {
        float: right;
        margin: 5px 5px 0 0;
        position: relative;
        width: 30px;
        height: 30px;
    }

    .search-block {
        z-index: 5;
        background: url('/_images/admin/search-bg.png') 3px center no-repeat #ffffff;
        width: 0;
        padding: 0 14px;
        cursor: pointer;
        position: absolute;
        top: 0;
        right: 0;

        &:hover {
            border: 1px solid $colorLinkBase;
        }
    }
}

.shop-admin-content {
    padding: 50px 10px 10px 10px;
    overflow: hidden;

    &.sidebar-place {
        padding-right: 40px;
        position: relative;
    }
}

.shop-stepper {
    float: right;

    a {
        color: #444444;
        text-decoration: none;
        margin: 0 3px;

        &:hover {
            text-decoration: underline;
        }

        &.selected {
            color: $colorLinkBase;
        }
    }
}

table thead.fix-t {
    position: fixed;
}

.shop-toggle-block {
    margin: 0 0 10px 0;

    a.toggle {
        color: #444444;
        font-weight: bold;
        text-decoration: none;
        padding: 0 0 0 12px;
        background: url('/_images/admin/p_05.png') 0 10px no-repeat;
        display: inline-block;
        font-size: 18px;

        &:hover {
            text-decoration: underline;
        }
    }

    a.close{
        background: url('/_images/admin/p_05-c.png') 0 8px no-repeat;
    }

    .block {
        background-color: #ffffff;
        padding: 15px;
        margin: 0 0 21px 0;
        color: #444444;

        .select2-container.chzn-select {
            display: inline-block;
        }
    }

    td {
        color: #444444;
    }

    .caption {
        padding: 0 0 2px 0;
        color: #444444;
    }

    .description-images {
        float: right;
        width: 280px;
        margin: 29px 0 0 0;

        .inner-block {
            min-height: 80px;
            border: 1px solid #bbbbbb;
            background-color: #ffffff;
            padding: 10px 10px 5px 10px;
        }

        [type="text"] {
            margin: 0 0 5px 0;
        }
    }

    .description-blocks {
        margin: 0 300px 0 0;
    }

    label {
        display: inline-block;
        margin: 0 25px 0 0;
        color: #444444;
        cursor: pointer;

        input {
            float: left;
            margin: 0 5px 0 0;
        }
    }

    .features {
        border-collapse: collapse;

        td {
            padding: 0 5px 5px 0;
        }
    }

    .products-form-row {
        margin: 0 0 16px 0;
        display: table;
        width: 100%;

        .small-width {
            width: 80px;
        }

        .column {
            display: table-cell;
            vertical-align: middle;
            padding: 0 0 0 12px;
            @include box-sizing($bb);

            [type='email'],
            [type='tel'],
            [type='password'],
            [type='text'] {
                width: 100%;
            }

            &:first-child {
                padding: 0;
            }

            &.five {
                width: 20%;
            }

            &.four {
                width: 25%;
            }

            &.three {
                width: 33%;
            }

            &.two {
                width: 50%px;
            }
        }

        label {
            display: inline-block;
            margin: 0 0 0 12px;

            [type='checkbox'],
            [type='radio'] {
                display: inline-block;
                vertical-align: middle;
                float: none;
            }
        }
    }

    .description {
        font-size: 11px;
        color: #7e7e7e;
        padding: 0 0 8px 0;
    }
}

.uploadify {
    position: relative;
    margin-bottom: 1em;
}

.uploadify-button {
    width: 100%;
    text-align: center;
    background: #505050;
    color: #ffffff !important;
    font-size: $fontSizeBase - 2;
    cursor: pointer;
    padding: 3px 8px;
    text-decoration: none;
}

.uploadify:hover .uploadify-button {
    background: $colorLinkBase;
}

.uploadify-button.disabled {
    background-color: #D0D0D0;
    color: #808080;
}

.uploadify-queue {
    margin-bottom: 1em;
}

.uploadify-queue-item {
    background-color: #F5F5F5;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    margin-top: 5px;
    max-width: 350px;
    padding: 10px;
}

.uploadify-error {
    background-color: #FDE5DD !important;
}

.uploadify-queue-item .cancel a {
    background: url('/_images/uploadify-cancel.png') 0 0 no-repeat;
    float: right;
    height: 16px;
    text-indent: -9999px;
    width: 16px;
}

.uploadify-queue-item.completed {
    background-color: #E5E5E5;
}

.uploadify-progress {
    background-color: #E5E5E5;
    margin-top: 10px;
    width: 100%;
}

.uploadify-progress-bar {
    background-color: $colorLinkBase;
    height: 3px;
    width: 1px;
}

.shop-photos-block {
    .item {
        margin: 0 0 17px 0;

        img {
            border: 1px solid #dedede;
            margin: 5px 0 9px 0;
        }
    }
}

.shop-export-block {
    position: relative;
    margin: 0 0 20px 9px;
    min-width: 150px;

    a.link {
        text-decoration: none;
        padding: 0 11px 0 0;
        background: url('/_images/admin/p_06.png') center right no-repeat;
        position: relative;
        z-index: 11;

        &:hover span {
            border-bottom: 1px dotted $colorLinkBase;
        }
    }

    .options {
        position: absolute;
        line-height: $lineHeightBase + 6;
        border-top: 1px solid #f5f5f5;
        border-left: 1px solid #e2e2e2;
        box-shadow: 2px 3px 2px #a0a0a0;
        z-index: 10;
        bottom: -5px;
        left: -9px;
        padding: 0 6px 25px 6px;
        background-color: #ffffff;

        .block {
            border-bottom: 1px dotted #a8abb1;
            padding: 3px 0 1px 0;
        }

        a {
            display:block;
            color:#444444;
            text-decoration:none;
            padding:0 0 0 22px;

            &.csv {
                background:url('/_images/admin/icon_csv.png') 1px 5px no-repeat;
            }

            &.xml {
                background:url('/_images/admin/icon_xml.png') 1px 5px no-repeat;
            }

            &.xls {
                background:url('/_images/admin/icon_xls.png') 1px 5px no-repeat;
            }

            &:hover {
                color:$colorLinkBase;
            }
        }
    }
}

.shop-block {
    padding: 10px;
    background-color: #eff0f2;
    border: 1px solid #eff0f2;
    margin-bottom: 10px;

    &.import {
        background-image: url('/_images/admin/icon-import.png');
        background-position: top right;
        background-repeat: no-repeat;
    }

    &.export {
        background-image: url('/_images/admin/icon-export.png');
        background-position: top right;
        background-repeat: no-repeat;
    }
}

.shop-form-block {
    width: 49%;

    [type="text"],
    [type="date"],
    [type="tel"],
    [type="password"],
    textarea {
        width: 100%;
    }

    [type="text"].date {
        width: 180px;
        padding-right: 30px;
        background: url('/_images/admin/icon-date.png') right center no-repeat;
    }

    select {
        width: 300px;
    }
}

.js-loader {
    width: 100%;
    height: 5px;
    z-index: 1000;
    position: fixed;
    z-index: 100001;
    top: -3px;
    right: 0;
    background: url('/_images/admin/loading3.gif');
    box-shadow: 0 0 5px #222;
    display: none;
}

.shop-wait {
    background-color: rgba(255, 255, 255, 0.75);
    background-image: url('/_images/admin/logo-loading.gif');
    background-position: center center;
    background-repeat: no-repeat;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 100002;

    span {
        position: absolute;
        width: 100%;
        top: 50%;
        text-align: center;
        color: $colorLinkBase;
        font-size: $fontSizeBase + 2;
        font-weight: bold;
        margin: 70px 0 0 0;
        line-height: $lineHeightBase + 6;
    }
}

.ob-success,
.ob-error {
    background-color: #ffffff;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    padding: 70px 50px;
    z-index: 100002;
    overflow: hidden;
    @include transition-duration(2.0s);
    box-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
    @include box-sizing($bb);

    .text {
        display: block;
        text-align: center;
        color: #ffffff;
        font-size: $fontSizeBase + 2;
        font-weight: bold;
        line-height: $lineHeightBase + 6;
        opacity: 0;
        @include transition-duration(1.0s);
    }

    &:before {
        content: '';
        position: absolute;
        width: 128px;
        height: 128px;
        background: url('/_images/admin/icons-message.png');
        top: 50%;
        left: 50%;
        margin: -64px 0 0 -64px;
        @include transition-duration(2.0s);
        z-index: -1;
    }

    &.go {
        background-color: rgba(0, 162, 0, 0.75);

        &:before {
            opacity: 0;
            // #2012099594
            //@include scale(10);
        }

        .text {
            opacity: 1;
        }
    }

    &.out {
        bottom: -100%;
    }
}

.ob-success {
    &:before {
        background-position: left top;
    }

    &.go {
        background-color: rgba(0, 162, 0, 0.75);
    }
 }

.ob-error {
    &:before {
        background-position: left bottom;
    }

    &.go {
        background-color: rgba(255, 43, 43, 0.75);
    }
 }

.shop-filter-line {
    line-height: $lineHeightBase + 22;

    label {
        margin: 0 10px 0 0;
        display: inline-block;
        vertical-align: middle;
    }
}

.shop-admin-orderpage {
    h2 {
        font-size: $fontSizeBase + 6;
        color: #000000;
        font-weight: bold;
    }

    label {
        display: inline-block;
        vertical-align: top;
        width: 45%;
        border: 1px solid #cccccc;
        padding: 10px 10px 10px 32px;
        margin: 0 0 5px 0;
        cursor: pointer;
        position: relative;

        &:hover {
            border: 1px solid $colorLinkBase;
            background-color: #d3e2f4;
        }

        [type=checkbox] {
            position: absolute;
            margin: -6px 0 0 0;
            padding: 0;
            top: 50%;
            left: 10px;
        }

        strong {
            display: block;
            font-size: $fontSizeBase + 2;
            font-weight: normal;
            color: #444444;
            margin: 0 0 3px 0;
        }

        span.term {
            background-color: #cce8b9;
            padding: 5px 10px;
            color: #ffffff;
            display: inline-block;
            white-space: nowrap;
            border-radius: 1px;
            vertical-align: top;
            width: 100px;
            text-align: center;
        }

        &:hover span.term {
            background-color: #4FB10F;
        }

        span.price {
            background-color: #eebac6;
            padding: 5px 10px;
            color: #ffffff;
            display: inline-block;
            white-space: nowrap;
            border-radius: 1px;
            vertical-align: top;
            width: 100px;
            text-align: center;
        }

        &:hover span.price {
            background-color: #C3113C;
        }

        span.description {
            color: #444444;
            font-size: $fontSizeBase - 3;
            display: block;
            margin: 0 0 10px 0;
        }
    }

    .total {
        float: right;
        color: #ffffff;
        padding: 2px 10px;
        margin: 0 10px;
        font-size: $fontSizeBase + 2;
        background-color: #C3113C;
        border-radius: 1px;
    }
}

.element-placeholder {
    border: 1px dashed #f8c13d;
    height: 28px;
    background-color: #f8eed5;
    margin: 0 0 5px 0;
}

.shop-table-view {
    .element {
        display: inline-block;
        width: 140px;
        vertical-align: top;
        cursor: pointer;
        margin: 0 0 5px 0;

        input {
            position: absolute;
            margin: 0;
        }

        span {
            display: inline-block;
            margin: 0 0 0 20px;
        }
    }

    .view {
        padding: 10px 0 0 0;
        margin: 0 0 10px 0;
        border-top: 1px dotted #cccccc;
        border-bottom: 1px dotted #cccccc;

        table {
            border-collapse: collapse;
        }

        td {
            padding: 0 10px 10px 0;
        }
    }
}

.shop-settings-block {
    background-color: #ffffff;
    color: #444444;
    margin: 7px 0 21px 0;
    padding: 15px;

    table {
        border-collapse: collapse;
        width: 100%;
    }

    td {
        padding: 0 0 0 20px;
        color: #444444;

        &:first-child {
            padding: 4px 0 0 0;
            font-weight: bold;
            font-size: $fontSizeBase;
            width: 200px;
            vertical-align: top;
            color: #000000;
        }
    }

    .light {
        font-size: $fontSizeBase - 3;
        color: #898989;
        display: block;
        font-weight: normal;
    }

    label {
        display: inline-block;
        cursor: pointer;

        input {
            float: left;
            margin: 1px 5px 0 0;
        }

        strong {
            font-size: $fontSizeBase;
            color: #000000;
        }
    }

    .current-image {
        background-color: #cdcaca;
        padding: 10px;
        display: inline-block;
        vertical-align: middle;
        margin: 0 20px 0 0;

        img {
            max-width: 600px;
        }
    }
}

.shop-filter-panel {
    float: left;
    width: 220px;
    margin: -10px -220px -10px -10px;
    position: relative;

    .inner-pannel {
        position: relative;
        padding: 10px;
    }

    &.close .inner-pannel {
        padding: 0;
    }

    .filter-toggle {
        position: fixed;
        width: 30px;
        height: 100%;
        background-image: url("/_images/admin/p-07.png");
        background-position: center center;
        background-repeat: no-repeat;
        top: 0;
        left: 0;
        cursor: pointer;
        z-index: 1;
        padding: 82px 0 0 0;
        @include transition-duration(0.3s);

        &.open {
            left: 215px;
        }

        &:hover {
            background-color: $colorLinkBase;
        }
    }

    .element {
        margin: 0 0 10px 0;
        position: relative;
    }

    [type="text"],
    [type="password"],
    [type="date"] {
        width: 100%;
        border: 1px solid white;
        border-bottom: 1px solid $colorBorderBase;
    }

    select {
        width: 200px;
    }

    [type="submit"] {
        position: absolute;
        right: -160px;
        width: 150px;
        opacity: 0;
        @include transition-duration(0.3s);
        z-index: -1;

        &.show {
            opacity: 1;
            z-index: 5;
            box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);
        }

        &:active {
            top: auto;
            left: auto;
            right: -161px;
        }
    }

    .pp-item {
        padding: 5px;
        margin: -5px -5px 5px -5px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;

        &:hover,
        &.selected {
            background-color: #eeeded;
        }

        .name {
            font-weight: bold;
            font-size: $fontSizeBase - 1;

            &:hover {
                color: $colorLinkBase;
            }
        }

        .view {
            color: $colorLinkBase;
            font-size: $fontSizeBase - 3;
        }
    }

    .info {
        color: #444444;
        font-size: $fontSizeBase - 3;

        a {
            font-size: $fontSizeBase - 3;
        }
    }

    .ulist {
        label {
            display: block;
            padding: 4px 4px 4px 25px;
            font-weight: bold;
            color: #555555;
            position: relative;
            cursor: pointer;

            input {
                top: 7px;
                left: 5px;
                position: absolute;
                margin: 0;
            }

            a {
                font-weight: bold;
            }

            &.sub {
                margin: 0 0 0 20px;

                a {
                    font-weight: normal;
                }
            }

            &.level_1 {
                margin: 0;
            }

            &.level_2 {
                margin: 0 0 0 20px;
            }

            &.level_3 {
                margin: 0 0 0 40px;
            }

            em {
                float: right;
                font-style: normal;
            }
        }

        .wraped {
            word-break: break-all;
        }
    }

    .ulist-caption {
        margin: 0 0 10px 0;
        padding: 0 0 0 25px;
        position: relative;
        color: $colorLinkBase;
        font-weight: bold;

        input {
            top: 3px;
            left: 5px;
            position: absolute;
            margin: 0;
        }

        span {
            border-bottom: 1px dashed;
            cursor: pointer;

            &:hover {
                border-color: transparent;
            }
        }

        &.inlist {
            margin: 0;
        }
    }
}

.shop-filter-list {
    .item {
        position: relative;
